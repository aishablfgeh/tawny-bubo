#!/usr/bin/env boot

(set-env! :dependencies
          '[[uk.org.russet/tawny-owl "2.0.0"]
            [uk.org.russet/tawny-bubo "0.1.0"]])

(defn -main [& args]
  (load-file (first args)))

(require 'tawny.bubo.util)
(tawny.bubo.util/prepare *ns*)

(clojure.core/require '[clojure.core :as cc])
(cc/use 'tawny.bubo.core)
(cc/use 'tawny.english)

